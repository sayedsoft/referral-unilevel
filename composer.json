{
  "name": "sayedsoft/referral-unilevel",
  "description": "Unilevel referral system",
  "version": "1.4.0",
  "keywords": [
    "sayedsoft",
    "referral-unilevel",
    "referral",
    "ahmad yaman sayed",
    "laravel referral system"
  ],
  "homepage": "https://github.com/sayedsoft/referral-unilevel",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Yaman Sayed",
      "email": "yemen@sayedsoft.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "laravel/framework": ">=10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "Sayedsoft\\ReferralUnilevel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sayedsoft\\ReferralUnilevel\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sayedsoft\\ReferralUnilevel\\ReferralUnilevelServiceProvider"
      ],
      "aliases": {
        "ReferralUnilevel": "Sayedsoft\\ReferralUnilevel\\ReferralUnilevelFacade"
      }
    }
  }
}
